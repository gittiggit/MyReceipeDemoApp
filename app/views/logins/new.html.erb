<%= render "shared/page_title", title: "Login" %>

<div class="row">
 <div class="well col-md-6 col-md-offset-3">
     <%= form_tag login_path do %>
     <p>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
     </p> 
     <p>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
     </p> 
      <p>
      <%= submit_tag "Login", class:"btn btn-success" %>
     </p> 
     
     <% end %>
 </div>         
</div>    